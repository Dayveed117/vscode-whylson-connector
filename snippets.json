{
  "whylson-equality": {
    "prefix": "whylson-eq",
    "body": [
      "$LINE_COMMENT@ ${0:value} = ${1|int,nat,mutez,string,address|} ?"
    ],
    "description": "Equality annotation for next \"let\" expression"
  },
  "whylson-inequality": {
    "prefix": "whylson-ineq",
    "body": [
      "$LINE_COMMENT@ ${0:value} <> ${1|int,nat,mutez,string,address|} ?"
    ],
    "description": "Inequality annotation for next \"let\" expression"
  },
  "whylson-greater-than": {
    "prefix": "whylson-gt",
    "body": [
      "$LINE_COMMENT@ ${0:value} > ${1|int,nat,mutez|} ?"
    ],
    "description": "\"Greater than\" annotation for next \"let\" expression"
  },
  "whylson-greater-equals": {
    "prefix": "whylson-ge",
    "body": [
      "$LINE_COMMENT@ ${0:value} >= ${1|int,nat,mutez|} ?"
    ],
    "description": "\"Greater equals\" annotation for next \"let\" expression"
  },
  "whylson-lesser-than": {
    "prefix": "whylson-lt",
    "body": [
      "$LINE_COMMENT@ ${0:value} < ${1|int,nat,mutez|} ?"
    ],
    "description": "\"Lesser than\" annotation for next \"let\" expression"
  },
  "whylson-lesser-equals": {
    "prefix": "whylson-le",
    "body": [
      "$LINE_COMMENT@ ${0:value} <= ${1|int,nat,mutez|} ?"
    ],
    "description": "\"Lesser Equals\" annotation for next \"let\" expression"
  },
  "whylson-bound": {
    "prefix": "whylson-bound",
    "body": [
      "$LINE_COMMENT@ ${2:low} ${3|<=,<|} ${1|int,nat,mutez|} ${5|<=,<|} ${4:high} ?"
    ],
    "description": "\"Within bounds\" annotation for next \"let\" expression"
  }
}
